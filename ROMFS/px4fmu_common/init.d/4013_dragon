#!nsh
#
# DJI Flame Wheel F450
#
# Lorenz Meier <lorenz@px4.io>
#

set VEHICLE_TYPE mc
set MIXER dragon

if [ $AUTOCNF == yes ]
then
	# TODO REVIEW
	param set MC_ROLL_P 7.0
	param set MC_ROLLRATE_P 0.1
	param set MC_ROLLRATE_I 0.0
	param set MC_ROLLRATE_D 0.003
	param set MC_PITCH_P 7.0
	param set MC_PITCHRATE_P 0.1
	param set MC_PITCHRATE_I 0.0
	param set MC_PITCHRATE_D 0.003
	param set MC_YAW_P 2.8
	param set MC_YAWRATE_P 0.2
	param set MC_YAWRATE_I 0.1
	param set MC_YAWRATE_D 0.0
fi

set PWM_RATE 50
set PWM_DISARMED 1500
set PWM_MIN 1200
set PWM_MAX 1800
# must contain all the channels belong to the same group
set PWM_OUT 1234

set DRAGON_MOTOR_OUT 5678
set DRAGON_MOTOR_RATE 400
set DRAGON_MOTOR_DISARMED 900
set DRAGON_MOTOR_MIN 1100
set DRAGON_MOTOR_MAX 1900
